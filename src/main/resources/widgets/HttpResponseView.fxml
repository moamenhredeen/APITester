<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TabPane xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         fx:controller="com.moamen.controllers.HttpResponseView"
         prefHeight="400.0" prefWidth="600.0"
         styleClass="outputConsole"
         tabClosingPolicy="UNAVAILABLE">
    <tabs>
        <Tab styleClass="outputConsoleTab" text="Raw">
            <content>
                <TextArea fx:id="outputRaw" editable="false" styleClass="outputConsoleTextArea"/>
            </content>
        </Tab>
        <Tab styleClass="outputConsoleTab" text="Response Headers">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
            </content>
        </Tab>
        <Tab styleClass="outputConsoleTab" text="Response Body">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
            </content>
        </Tab>
    </tabs>
</TabPane>
